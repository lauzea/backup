SRC=		main.c\

NAME=		eval_expr

OBJ=		$(SRC:.c=.o)

all:		$(NAME)

$(NAME):	$(OBJ)
		gcc -o $(NAME) $(OBJ)

clean:		rm -f $(OBJ)

fclean:		clean
		rm -f $(NAME)

re:		fclean all
